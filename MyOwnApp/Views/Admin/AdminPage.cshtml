
@{
    Layout = null;
}
@model MyOwnApp.ViewModels.AdminModel
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>AdminPage</title>
    <link href="~/css/font-face.css" rel="stylesheet" media="all">
    <link href="~/vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
    <link href="~/vendor/font-awesome-5/css/fontawesome-all.min.css" rel="stylesheet" media="all">
    <link href="~/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">

    <!-- Bootstrap CSS-->
    <link href="~/vendor/bootstrap-4.1/bootstrap.min.css" rel="stylesheet" media="all">

    <!-- Vendor CSS-->
    <link href="~/vendor/animsition/animsition.min.css" rel="stylesheet" media="all">
    <link href="~/vendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet" media="all">
    <link href="~/vendor/wow/animate.css" rel="stylesheet" media="all">
    <link href="~/vendor/css-hamburgers/hamburgers.min.css" rel="stylesheet" media="all">
    <link href="~/vendor/slick/slick.css" rel="stylesheet" media="all">
    <link href="~/vendor/select2/select2.min.css" rel="stylesheet" media="all">
    <link href="~/vendor/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" media="all">

    <!-- Main CSS-->
    <link href="~/css/theme.css" rel="stylesheet" media="all">
</head>
<body class="animsition">
    <div class="page-wrapper">




        <!-- PAGE CONTENT-->
        <div class="page-content--bgf7">


            <!-- WELCOME-->
            <section class="welcome p-t-10">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <h1 class="title-4">
                                Welcome back
                                <span>@Model.AdminName!</span>
                            </h1>
                            <hr class="line-seprate">
                        </div>
                    </div>
                </div>
            </section>
            <!-- END WELCOME-->
            <!-- STATISTIC-->
            <section class="statistic statistic2">
                <div class="container">
                    <div class="row">
                        <div class="col-md-6 col-lg-3">
                            <div class="statistic__item statistic__item--green">
                                <h2 class="number">@Model.OnlineUsers</h2>
                                <span class="desc">members online</span>
                                <div class="icon">
                                    <i class="zmdi zmdi-account-o"></i>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6 col-lg-3">
                            <div class="statistic__item statistic__item--orange">
                                <h2 class="number">@Model.AllTimeSold</h2>
                                <span class="desc">all time profit</span>
                                <div class="icon">
                                    <i class="zmdi zmdi-shopping-cart"></i>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6 col-lg-3">
                            <div class="statistic__item statistic__item--blue">
                                <h2 class="number">@Model.WeekSoldCount</h2>
                                <span class="desc">this week</span>
                                <div class="icon">
                                    <i class="zmdi zmdi-calendar-note"></i>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6 col-lg-3">
                            <div class="statistic__item statistic__item--red">
                                <h2 class="number">$@Model.WeekSold</h2>
                                <span class="desc">total earnings</span>
                                <div class="icon">
                                    <i class="zmdi zmdi-money"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!-- END STATISTIC-->
            <!-- STATISTIC CHART-->
            <section class="statistic-chart">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <h3 class="title-5 m-b-35">statistics</h3>
                        </div>
                    </div>
                    <div class="row">
                        <!-- <div class="col-md-6 col-lg-4"> -->
                        <div class="col-md-12 col-lg-12">
                            <!-- CHART-->
                            <div class="statistic-chart-1">
                                <h3 class="title-3 m-b-30">chart</h3>
                                <div class="chart-wrap">
                                    <!-- grafic point -->
                                    <canvas id="myLineChart"></canvas>
                                </div>
                                <div class="statistic-chart-1-note">
                                    <span class="big">10,368</span>
                                    <span>/ 16220 items sold</span>
                                </div>
                            </div>
                            <!-- END CHART-->
                        </div>
                        <div class="col-md-12 col-lg-12">
                            <!-- CHART-->
                            <div class="statistic-chart-1">
                                <h3 class="title-3 m-b-30">chart</h3>
                                <div class="chart-wrap">
                                    <!-- grafic point -->
                                    <canvas id="myLineChart2"></canvas>
                                </div>
                                <div class="statistic-chart-1-note">
                                    <span class="big">10,368</span>
                                    <span>/ 16220 items sold</span>
                                </div>
                            </div>
                            <!-- END CHART-->
                        </div>
                        <div class="col-lg-1">
                        </div>
                        <!-- <div class="col-md-6 col-lg-4"> -->
                        <div class="col-md-12 col-lg-4">
                            <!-- TOP CAMPAIGN-->
                            <div class="top-campaign">
                                <h3 class="title-3 m-b-30">top campaigns</h3>
                                <div class="table-responsive">
                                    <table class="table table-top-campaign">
                                        <tbody>
                                            <tr>
                                                <td>1. @Model.TopProducers[0].NameOfProducer</td>
                                                <td>$@Model.TopProducers[0].SoldSumm</td>
                                            </tr>
                                            @*<tr>
                                            <td>2. @Model.TopProducers[1].NameOfProducer</td>
                                            <td>$@Model.TopProducers[1].SoldSumm</td>
                                        </tr>
                                        <tr>
                                            <td>3. @Model.TopProducers[2].NameOfProducer</td>
                                            <td>$@Model.TopProducers[2].SoldSumm</td>
                                        </tr>
                                        <tr>
                                            <td>4. @Model.TopProducers[3].NameOfProducer</td>
                                            <td>$@Model.TopProducers[3].SoldSumm</td>
                                        </tr>
                                        <tr>
                                            <td>5. @Model.TopProducers[4].NameOfProducer</td>
                                            <td>$@Model.TopProducers[4].SoldSumm</td>
                                        </tr>*@
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                            <!-- END TOP CAMPAIGN-->
                        </div>
                        <!-- <div class="col-md-6 col-lg-4"> -->
                        <div class="col-md-12 col-lg-6">
                            <!-- CHART PERCENT-->
                            <div class="chart-percent-2">
                                <h3 class="title-3 m-b-15">chart by %</h3>
                                <div class="chart-wrap">
                                    <canvas id="myCircleChart"></canvas>
                                </div>

                            </div>
                            <!-- END CHART PERCENT-->
                        </div>
                    </div>
                </div>
            </section>
            <!-- END STATISTIC CHART-->

            @await Html.PartialAsync("AddProduct", Model.AddProductModel)
            @await Html.PartialAsync("AddType", Model.AddTypeModel)
            @await Html.PartialAsync("AddModel", Model.AddModelModel)
            @await Html.PartialAsync("AddProducer", Model.AddProducerModel)
            @await Html.PartialAsync("AddDimension", Model.AddDimensionModel)
            @await Html.PartialAsync("AddMaterial", Model.AddMaterialModel)

            <!-- DATA TABLE-->
            <section class="p-t-20">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <h3 class="title-5 m-b-35">data table</h3>
                            <div class="table-data__tool">
                                <div class="table-data__tool-left">
                                    <div class="rs-select2--light rs-select2--md">
                                        <select class="js-select2" name="property">
                                            <option selected="selected">All Properties</option>
                                            <option value="">Option 1</option>
                                            <option value="">Option 2</option>
                                        </select>
                                        <div class="dropDownSelect2"></div>
                                    </div>
                                    <div class="rs-select2--light rs-select2--sm">
                                        <select class="js-select2" name="time">
                                            <option selected="selected">Today</option>
                                            <option value="">3 Days</option>
                                            <option value="">1 Week</option>
                                        </select>
                                        <div class="dropDownSelect2"></div>
                                    </div>
                                    <button class="au-btn-filter">
                                        <i class="zmdi zmdi-filter-list"></i>filters
                                    </button>
                                </div>
                                <div class="table-data__tool-right">
                                    <button class="au-btn au-btn-icon au-btn--green au-btn--small">
                                        <i class="zmdi zmdi-plus"></i>add item
                                    </button>
                                    <div class="rs-select2--dark rs-select2--sm rs-select2--dark2">
                                        <select class="js-select2" name="type">
                                            <option selected="selected">Export</option>
                                            <option value="">Option 1</option>
                                            <option value="">Option 2</option>
                                        </select>
                                        <div class="dropDownSelect2"></div>
                                    </div>
                                </div>
                            </div>
                            <div class="table-responsive table-responsive-data2">
                                <table class="table table-data2">
                                    <thead>
                                        <tr>
                                            <th></th>
                                            <th>name</th>
                                            <th>email</th>
                                            <th>description</th>
                                            <th>date</th>
                                            <th>status</th>
                                            <th>price</th>
                                            <th></th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @{
                                            foreach (var item in Model.Orders)
                                            {
                                                <tr class="tr-shadow">
                                                    <td>
                                                        <label class="au-checkbox">
                                                            <input type="checkbox">
                                                            <span class="au-checkmark"></span>
                                                        </label>
                                                    </td>
                                                    <td>@item.Name</td>
                                                    <td>
                                                        <span class="block-email">@item.Email</span>
                                                    </td>
                                                    <td class="desc">@item.Description</td>
                                                    <td>@item.Date</td>
                                                    <td>
                                                        @{
                                                            if (item.Status == "Denied")
                                                            {
                                                                <span class="status--denied">Denied</span>
                                                            }
                                                            else
                                                            {
                                                                <span class="status--process">Processed</span>
                                                            }
                                                        }
                                                    </td>
                                                    <td>$@item.Price</td>
                                                    <td>
                                                        <div class="table-data-feature">

                                                            <button class="item" data-toggle="tooltip" data-placement="top"
                                                                    title="Edit">
                                                                <i class="zmdi zmdi-edit"></i>
                                                            </button>
                                                            <button class="item" data-toggle="tooltip" data-placement="top"
                                                                    title="Delete">
                                                                <i class="zmdi zmdi-delete"></i>
                                                            </button>
                                                            <button class="item" data-toggle="tooltip" data-placement="top"
                                                                    title="More">
                                                                <i class="zmdi zmdi-more"></i>
                                                            </button>
                                                        </div>
                                                    </td>
                                                </tr>
                                            }
                                        }
                                    </tbody>
                                </table>
                            </div>
                            <div class="table-responsive table-responsive-data2">
                                <table class="table table-data2">
                                    <thead>
                                        <tr>
                                            <th>id</th>
                                            <th>name</th>
                                            <th>description</th>
                                            <th>available</th>
                                            <th>characteristics</th>
                                            <th>price</th>
                                            <th></th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @{
                                            foreach (var item in Model.Products)
                                            {
                                                <tr class="tr-shadow">

                                                    <td>@item.Id</td>
                                                    <td>
                                                        <span class="block-email">@item.Name</span>
                                                    </td>
                                                    <td class="desc">@item.Description</td>
                                                    <td>@item.AvailableCount</td>
                                                    <td>@item.Characteristics</td>
                                                    <td>$@item.Price</td>
                                                    <td>
                                                        <div class="table-data-feature">

                                                            <button class="item" data-toggle="tooltip" data-placement="top"
                                                                    title="Edit">
                                                                <i class="zmdi zmdi-edit"></i>
                                                            </button>
                                                            <button class="item" data-toggle="tooltip" data-placement="top"
                                                                    title="Delete">
                                                                <i class="zmdi zmdi-delete"></i>
                                                            </button>
                                                            <button class="item" data-toggle="tooltip" data-placement="top"
                                                                    title="More">
                                                                <i class="zmdi zmdi-more"></i>
                                                            </button>
                                                        </div>
                                                    </td>
                                                </tr>
                                            }
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!-- END DATA TABLE-->
            <!-- COPYRIGHT-->
            <section class="p-t-60 p-b-20">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="copyright">
                                <p>Copyright © 2018. All rights reserved. </p>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!-- END COPYRIGHT-->
        </div>

    </div>

    <!-- Jquery JS-->
    <script src="~/vendor/jquery-3.2.1.min.js"></script>
    <!-- Bootstrap JS-->
    <script src="~/vendor/bootstrap-4.1/popper.min.js"></script>
    <script src="~/vendor/bootstrap-4.1/bootstrap.min.js"></script>
    <!-- Vendor JS       -->
    <script src="~/vendor/slick/slick.min.js"></script>
    <script src="~/vendor/wow/wow.min.js"></script>
    <script src="~/vendor/animsition/animsition.min.js"></script>
    <script src="~/vendor/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>
    <script src="~/vendor/counter-up/jquery.waypoints.min.js"></script>
    <script src="~/vendor/counter-up/jquery.counterup.min.js"></script>
    <script src="~/vendor/circle-progress/circle-progress.min.js"></script>
    <script src="~/vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="~/vendor/chartjs/Chart.bundle.min.js"></script>
    <script src="~/vendor/select2/select2.min.js"></script>
    <!-- Main JS-->
    <script src="~/js/main.js"></script>
    <script>
        var myLineChart = new Chart(document.getElementById("myLineChart"), {
            type: 'line',
            data: {
                labels: ["January", "February", "March", "April", "May", "June",
            "July", "August", "September", "October", "November", "December"],
                datasets: [{
                    label: "My First ",
                    data: [@Model.ChartValue[0], @Model.ChartValue[1],@Model.ChartValue[2], @Model.ChartValue[3],
                        @Model.ChartValue[4], @Model.ChartValue[5], @Model.ChartValue[6],@Model.ChartValue[7], @Model.ChartValue[8],
                        @Model.ChartValue[9], @Model.ChartValue[10], @Model.ChartValue[11]],
                    backgroundColor: [
                        'rgba(36, 116, 36, .7)',
                    ],
                    borderColor: [
                        'rgba(7, 5, 30, .7)',
                    ],
                    pointHoverBackgroundColor: [
                        'rgba(36, 116, 36, .7)',
                        'rgba(36, 116, 36, .7)',
                        'rgba(36, 116, 36, .7)',
                        'rgba(36, 116, 36, .7)',
                    ],
                    pointHoverBorderColor: [
                        'rgba(7, 5, 30, .7)',
                        'rgba(7, 5, 30, .7)',
                        'rgba(7, 5, 30, .7)',
                        'rgba(7, 5, 30, .7)',
                    ],

                    borderWidth: 2
                },
                //{
                //    label: "My Second ",
                //    data: [2800,8000,3000, 4800, 4000, 1900,9000,5000,3500,7100,1000,1500],
                //    backgroundColor: [
                //        'rgba(51, 40, 204, .7)',
                //    ],
                //    borderColor: [
                //        'rgba(14, 41, 14, .7)',
                //    ],
                //    pointHoverBackgroundColor: [
                //        'rgba(51, 40, 204, .7)',
                //        'rgba(51, 40, 204, .7)',
                //        'rgba(51, 40, 204, .7)',
                //        'rgba(51, 40, 204, .7)',
                //    ],
                //    pointHoverBorderColor: [
                //        'rgba(14, 41, 14, .7)',
                //        'rgba(14, 41, 14, .7)',
                //        'rgba(14, 41, 14, .7)',
                //        'rgba(14, 41, 14, .7)',
                //    ],
                //    borderWidth: 2
                //}
                ]
            },
            options: {
                responsive: true
            }
        });
        var myLineChart2 =new Chart(document.getElementById("myLineChart2"), {
              type: 'line',
              data: {
                labels: ["January", "February", "March", "April", "May", "June",
            "July", "August", "September", "October", "November", "December"],
                datasets: [{
                    data: [@Model.ChartYearTypes[0].ChartValue[0],@Model.ChartYearTypes[0].ChartValue[1],@Model.ChartYearTypes[0].ChartValue[2],
                        @Model.ChartYearTypes[0].ChartValue[3],@Model.ChartYearTypes[0].ChartValue[4],@Model.ChartYearTypes[0].ChartValue[5],
                        @Model.ChartYearTypes[0].ChartValue[6],@Model.ChartYearTypes[0].ChartValue[7],@Model.ChartYearTypes[0].ChartValue[8],
                        @Model.ChartYearTypes[0].ChartValue[9],@Model.ChartYearTypes[0].ChartValue[10],@Model.ChartYearTypes[0].ChartValue[11]],
                    label: "@Model.ChartYearTypes[0].Year",
                    borderColor: "#8e5ea2",
                    fill: false
                  }@*,{
                    data: [@Model.ChartYearTypes[1].ChartValue[0],@Model.ChartYearTypes[1].ChartValue[1],@Model.ChartYearTypes[1].ChartValue[2],
                        @Model.ChartYearTypes[1].ChartValue[3],@Model.ChartYearTypes[1].ChartValue[4],@Model.ChartYearTypes[1].ChartValue[5],
                        @Model.ChartYearTypes[1].ChartValue[6],@Model.ChartYearTypes[1].ChartValue[7],@Model.ChartYearTypes[1].ChartValue[8],
                        @Model.ChartYearTypes[1].ChartValue[9],@Model.ChartYearTypes[1].ChartValue[10],@Model.ChartYearTypes[1].ChartValue[11]],
                    label: "@Model.ChartYearTypes[1].Year",
                    borderColor: "#3e95cd",
                    fill: false
                  },{
                    data: [@Model.ChartYearTypes[2].ChartValue[0],@Model.ChartYearTypes[2].ChartValue[1],@Model.ChartYearTypes[2].ChartValue[2],
                        @Model.ChartYearTypes[2].ChartValue[3],@Model.ChartYearTypes[2].ChartValue[4],@Model.ChartYearTypes[2].ChartValue[5],
                        @Model.ChartYearTypes[2].ChartValue[6],@Model.ChartYearTypes[2].ChartValue[7],@Model.ChartYearTypes[2].ChartValue[8],
                        @Model.ChartYearTypes[2].ChartValue[9],@Model.ChartYearTypes[2].ChartValue[10],@Model.ChartYearTypes[2].ChartValue[11]],
                    label: "@Model.ChartYearTypes[2].Year",
                    borderColor: "#3cba9f",
                    fill: false
                  }, {
                    data: [@Model.ChartYearTypes[3].ChartValue[0],@Model.ChartYearTypes[3].ChartValue[1],@Model.ChartYearTypes[3].ChartValue[2],
                        @Model.ChartYearTypes[3].ChartValue[3],@Model.ChartYearTypes[3].ChartValue[4],@Model.ChartYearTypes[3].ChartValue[5],
                        @Model.ChartYearTypes[3].ChartValue[6],@Model.ChartYearTypes[3].ChartValue[7],@Model.ChartYearTypes[3].ChartValue[8],
                        @Model.ChartYearTypes[3].ChartValue[9],@Model.ChartYearTypes[3].ChartValue[10],@Model.ChartYearTypes[3].ChartValue[11]],
                    label: "@Model.ChartYearTypes[3].Year",
                    borderColor: "#e8c3b9",
                    fill: false
                  }, {
                    data: [@Model.ChartYearTypes[4].ChartValue[0],@Model.ChartYearTypes[4].ChartValue[1],@Model.ChartYearTypes[4].ChartValue[2],
                        @Model.ChartYearTypes[4].ChartValue[3],@Model.ChartYearTypes[4].ChartValue[4],@Model.ChartYearTypes[4].ChartValue[5],
                        @Model.ChartYearTypes[4].ChartValue[6],@Model.ChartYearTypes[4].ChartValue[7],@Model.ChartYearTypes[4].ChartValue[8],
                        @Model.ChartYearTypes[4].ChartValue[9],@Model.ChartYearTypes[4].ChartValue[10],@Model.ChartYearTypes[4].ChartValue[11]],
                    label: "@Model.ChartYearTypes[4].Year",
                    borderColor: "#c45850",
                    fill: false
                  }*@
                ]
              },
              options: {
                title: {
                  display: true,
                  text: 'Display all times company profits'
                }
              }
            });
        var myCircleChart = new Chart(document.getElementById("myCircleChart"), {
            type: 'doughnut',
            data: {
                labels: ["@Model.ChartProductTypes[0].NameOfType", "@Model.ChartProductTypes[1].NameOfType", "@Model.ChartProductTypes[2].NameOfType", "@Model.ChartProductTypes[3].NameOfType", "@Model.ChartProductTypes[4].NameOfType"],
                datasets: [{
                    data: [@Model.ChartProductTypes[0].SoldSumm, @Model.ChartProductTypes[1].SoldSumm, @Model.ChartProductTypes[2].SoldSumm, @Model.ChartProductTypes[3].SoldSumm, @Model.ChartProductTypes[4].SoldSumm],
                    backgroundColor: ["#F7464A", "#46BFBD", "#FDB45C", "#949FB1", "#4D5360"],
                    hoverBackgroundColor: ["#FF5A5E", "#5AD3D1", "#FFC870", "#A8B3C5", "#616774"]
                }]
            },
            options: {
                responsive: true
            }
        });</script>
</body>
</html>
