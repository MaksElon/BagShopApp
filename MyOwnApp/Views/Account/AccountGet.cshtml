
@{
    Layout = "_Layout";
}
@model MyOwnApp.ViewModels.AccountGetModel

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>AccountGet</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" />
    <link href="~/css/styleLog.css" rel="stylesheet" />

</head>
<body>
    <div class="login-wrap">
        <div class="login-html">
            <input id="tab-1" type="radio" name="tab" class="sign-in" checked><label for="tab-1" class="tab">Sign In</label>
            <input id="tab-2" type="radio" name="tab" class="sign-up"><label for="tab-2" class="tab">Sign Up</label>
            <div class="login-form">

                @await Html.PartialAsync("Login", Model.LoginViewModel)
                @await Html.PartialAsync("Register", Model.RegisterViewModel)

            </div>
        </div>
    </div>
    <script src="https://code.jquery.com/jquery-2.2.4.min.js"></script>
    <script src="~/js/validateRegister.js"></script>

</body>
</html>
